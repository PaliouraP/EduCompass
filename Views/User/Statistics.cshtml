@{
    ViewData["Title"] = "Statistics";
}

@section Styles {
    <link rel="stylesheet" href="~/css/statistics.css" />
    <link rel="stylesheet" href="~/css/horizontal_bar_chart.css" />
}


<div class="chart-wrap vertical">
    <h2 class="mt-5">Σύγκριση προσανατολισμών</h2>

    <div class="row my-5">
        <div class="col-5">
            <h6 class="mb-4">Βαθμοί μαθημάτων</h6>
            <div class="grid">
                <div class="bar" style="--bar-value:85%;" data-name="Προσανατολισμός 1" title="Προσανατολισμός 1: 85%"></div>
                <div class="bar" style="--bar-value:45%;" data-name="Προσανατολισμός 2"></div>
